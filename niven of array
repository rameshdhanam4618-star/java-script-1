<script>
    var a = [56,78,98,90]
    for(i=0; i<a.length; i++)
    {
      var num = a[i]
    var t = num
    var sum = 0
    while(t > 0)
    {
        sum= t % 10
        t= parseInt(t / 10)
    }
    if (num % sum ==0)
    {
        document.write("niven number<br>")
    }
    else{
        document.write(" not niven number<br>")
    }
}
</script>
