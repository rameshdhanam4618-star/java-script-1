<script>
    var a = [23,34,45,56]
    for(i = 0; i<a.length; i++)
    {
        var num =a[i]
    var sq = num * num
    var sum = 0
    while(sq > 0)
    {
        sum = sq % 10
        sq = parseInt(sq /10)
}
if (sum==num)
{
    document.write(" neon number<br>")
}
else{
    document.write("not neon n umber<br>")
}
    }
</script>
