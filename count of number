<script>
    var c=0
    for (var i =1; i < 10; i++)
    {
        if (i%2==0){
            c++
        }
    }
    document.write(c + "even")
</script>

<script>
    var c=0
    for (var i =1; i <= 100; i++)
    {
        if (i<50){
            c++
        }
    }
    document.write(c + "number are greater than 50:")
</script>

<script>
    var c=0
    for (var i =1; i < 10; i++)
    {
        if (i%2!=0){
            c++
        }
    }
    document.write(c + "odd")
</script>
<script>
    var c=0
    for (var i =1; i <=50; i++)
    {
        if (i%5==0){
            c++
        }
    }
    document.write(c + "count for divisible by 5")
</script>

<script>
    var c=0
    for (var i =1; i < 100; i++)
    {
        if (i%7==0 || i%10==7){
            c++
        }
    }
    document.write(c + "Buzz")
</script>

<script>
    var c=0
    for (var a =1; a < 100; a++)
    {
        if (a%10==0){
            c++
        }
    }
    document.write(a + "Buzz")
</script>

<script>
    var c=0
    for (var i =1; i <= 100; i++)
    {
        if (i%3==0){
            c++
        }
    }
    document.write(c + "Divisible by 3")
</script>

<script>
    var c = 0
    for (var i = 1; i <= 200; i++) {
        if (i % 10 == 0) {
            c++
        }
    }
    document.write(c + " numbers multiples of 10")
</script>

<script>
    var c=0
    for (var i =1; i < 100; i++)
    {
        if (i%10==5){
            c++
        }
    }
    document.write(c + " numbers end with 5")
</script>

<script>
    var c=0
    for (var i =1; i < 50; i++)
    {
        if (i<20){
            c++
        }
    }
    document.write(c + " numbers are less than 20")
</script>




