<script>
    var a = [123, 78, 71, 34]
    for (var i = 0; i < a.length; i++) {
        var num = a[i]
        var sum = 0
        var product = 1
        while (num > 0) {
            var d = num % 10
            sum = sum + d
            product =product * d
            num = parseInt(num / 10)
        }
        if (sum == product) {
            document.write(" spy number <br>")
        }
        else {
            document.write("not a spy number<br>")
        }
    }
</script>
